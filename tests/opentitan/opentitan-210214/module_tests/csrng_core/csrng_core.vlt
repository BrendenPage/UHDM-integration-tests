`verilator_config

// Warnings that are thrown by original verilator

lint_off -rule WIDTH -file "*" -match "Operator GTE expects 32 bits on the LHS, but LHS's VARREF 'interate_ctr_q' generates 2 bits."
lint_off -rule WIDTH -file "*" -match "Operator SHIFTL expects 512 bits on the LHS, but LHS's VARREF 'concat_outblk_q' generates 384 bits."
lint_off -rule WIDTH -file "*" -match "Operator GTE expects 32 bits on the LHS, but LHS's VARREF 'concat_ctr_q' generates 2 bits."
lint_off -rule WIDTH -file "*" -match "Operator COND expects 808 bits on the Conditional True, but Conditional True's REPLICATE generates 439 bits."
lint_off -rule WIDTH -file "*" -match "Operator ASSIGNW expects 3 bits on the Assign RHS, but Assign RHS's DIVS generates 32* bits."
lint_off -rule WIDTH -file "*" -match "Operator SHIFTL expects 384 bits on the LHS, but LHS's VARREF 'wdata_i' generates 32 bits."
lint_off -rule WIDTH -file "*" -match "Operator EQ expects 32* bits on the LHS, but LHS's VARREF 'depth_q' generates 5 bits."
lint_off -rule WIDTH -file "*" -match "Operator ASSIGNW expects 4 bits on the Assign RHS, but Assign RHS's SUB generates 32 bits."

